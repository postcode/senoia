<table>
  <tbody>
    <tr>
      <td>Name</td>
      <td>Start Date</td>
      <td>Attendance</td>
      <td>Event Type</td>
    </tr>
<% Plan.all.each do |plan| %>
  <tr>
    <td><%= plan.name %></td>
    <td><%= plan.start_date %></td>
    <td><%= plan.attendance %></td>
    <td><%= plan.event_type %></td>
  </tr>
<% end %>
</tbody>
</table>