<p>A new medical plan has been submitted:</p>

<table>
  <tr>
    <th>Name</th>
    <td><%= @plan.name %></td>
  </tr>
  <tr>
    <th>Event Type</th>
    <td><%= @plan.event_type %></td>
  </tr>
  <tr>
    <th>Start Date</th>
    <td><%= @plan.end_date.present? ? l(@plan.start_date) : "No Date" %></td>
  </tr>
  <tr>
    <th>End Date</th>
    <td><%= @plan.end_date.present? ? l(@plan.end_date) : "No Date" %></td>
  </tr>
  <tr>
    <th>Attendance</th>
    <td><%= @plan.attendance %></td>
  </tr>
</table>

<p><%= link_to "View plan", @plan, target: "_blank" %></p>
