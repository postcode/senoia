<div class="row">
  <div class="large-12 columns">
    <%= simple_form_for @permitter do |f| %>
      <%= f.input :name %>  
      <%= f.input :address %>
      <%= f.input :email, as: "email" %>
      <%= f.input :phone_number %>
      
      <%= f.submit class: "button" %>

    <% end %> 
  </div>
</div>
